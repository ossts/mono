	date.anytime()
{{~else utilsEquals format 'date'}}
	date.anytime()
{{~else utilsEquals format 'time'}}
	date.anytime()
{{~else utilsEquals format 'uri'}}
	internet.url()
{{~else utilsEquals format 'uuid'}}
	string.uuid()
