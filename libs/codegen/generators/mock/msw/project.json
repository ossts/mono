{
  "name": "codegen-generators-mock-msw",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/codegen/generators/mock/msw/src",
  "projectType": "library",
  "tags": ["scope:ts", "type:lib", "project:codegen"],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/codegen/generators/mock/msw"
      },
      "dependsOn": ["precompile-templates"]
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/libs/codegen/generators/mock/msw"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../../../../coverage/libs/codegen/generators/mock/msw"
      },
      "dependsOn": ["precompile-templates"]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "dependsOn": ["precompile-templates"]
    },
    "precompile-templates": {
      "executor": "@ossts/plugin-codegen:precompile-templates",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "libs/codegen/generators/mock/msw/src/lib/precompiled-templates"
      }
    }
  }
}
