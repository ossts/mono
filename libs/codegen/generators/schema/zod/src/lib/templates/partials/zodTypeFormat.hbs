{{~#utilsEquals export 'dictionary'~}}
	{{>zodReferenceValue name=type}}
{{~else utilsEquals export 'reference'~}}
	{{~#utilsStartsWith base "'"~}}
		z.literal({{base}})
	{{~else~}}
		{{>zodReferenceValue name=type}}
	{{~/utilsStartsWith~}}
{{~else utilsEquals format 'date-time'~}}
	.string().datetime()
{{~else utilsEquals format 'date'~}}
	.date()
{{~else utilsEquals format 'time'~}}
	.time()
{{~else utilsEquals format 'uri'~}}
	.string().url()
{{~else utilsEquals format 'uuid'~}}
	.string().uuid()
{{~else utilsEquals type 'binary'~}}
	.object()
{{~else utilsEquals type 'string'~}}
	.string()
{{~else utilsEquals type 'any'~}}
	.any()
{{~else utilsEquals type 'null'~}}
	.null()
{{~else utilsEquals type 'undefined'~}}
	.undefined()
{{~else~}}
	.{{type}}()
{{~/utilsEquals~}}