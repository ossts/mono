{{#utilsIfSome description deprecated}}
/**
{{#if description}}
 * {{{utilsEscapeComment description}}}
{{/if}}
{{#if deprecated}}
 * @deprecated
{{/if}}
 */
{{/utilsIfSome}}
export type {{{utilsNameWithNS name}}} = {
	{{#each properties}}
	{{#utilsIfSome description deprecated}}
	/**
	{{#if description}}
	 * {{{utilsEscapeComment description}}}
	{{/if}}
	{{#if deprecated}}
	 * @deprecated
	{{/if}}
	 */
	{{/utilsIfSome}}
	{{>utilsReadOnly}}{{{name}}}{{>utilsRequired}}: {{>utilsType parent=../name}};
	{{/each}}
};

export namespace {{{utilsNameWithNS name}}} {
	{{>exportNoPrimaryKey}}

	{{#if enums}}
	{{#unless @root.settings.useUnionTypes}}
	{{#each enums}}
	{{#if description}}
	/**
	 * {{{utilsEscapeComment description}}}
	 */
	{{/if}}
	export enum {{{name}}} {
		{{#each enum}}
		{{{name}}} = {{{value}}},
		{{/each}}
	}

	{{/each}}
	{{/unless}}
	{{/if}}

}