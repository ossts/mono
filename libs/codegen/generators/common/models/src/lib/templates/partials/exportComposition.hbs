{{#utilsIfSome description deprecated}}
/**
{{#if description}}
 * {{{utilsEscapeComment description}}}
{{/if}}
{{#if deprecated}}
 * @deprecated
{{/if}}
 */
{{/utilsIfSome}}
export type {{{utilsNameWithNS name}}} = {{>utilsType parent=name}};

export namespace {{{utilsNameWithNS name}}} {
	{{>exportNoPrimaryKey}}

	{{#if enums}}
	{{#unless @root.settings.useUnionTypes}}
		{{#each enums}}
		{{#utilsIfSome description deprecated}}
		/**
		{{#if description}}
		* {{{utilsEscapeComment description}}}
		{{/if}}
		{{#if deprecated}}
		* @deprecated
		{{/if}}
		*/
		{{/utilsIfSome}}
		export enum {{{name}}} {
			{{#each enum}}
			{{{name}}} = {{{value}}},
			{{/each}}
		}

		{{/each}}
	{{/unless}}
	{{/if}}

}